{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/django/django.json",
            "log_group_name": "/app/HOST_CLASS/django",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/django/django_celery_worker.json",
            "log_group_name": "/app/HOST_CLASS/celery/worker",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/django/django_celery_localhost.json",
            "log_group_name": "/app/HOST_CLASS/celery/localhost",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/django/django_celery_priority.json",
            "log_group_name": "/app/HOST_CLASS/celery/priority",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/django/rabbitmq_stats.json",
            "log_group_name": "/app/HOST_CLASS/rabbitmq",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/nginx/access.log",
            "log_group_name": "/app/HOST_CLASS/nginx/access",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/nginx/error.log",
            "log_group_name": "/app/HOST_CLASS/nginx/error",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/launch_host.log",
            "log_group_name": "/app/HOST_CLASS/launch-host",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/messages",
            "log_group_name": "/app/HOST_CLASS/system/messages",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/maillog",
            "log_group_name": "/app/HOST_CLASS/system/maillog",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/cron",
            "log_group_name": "/app/HOST_CLASS/system/cron",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/secure",
            "log_group_name": "/app/HOST_CLASS/system/secure",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/kernel",
            "log_group_name": "/app/HOST_CLASS/system/kernel",
            "log_stream_name": "HOST_CLASS"
          },
          {
            "file_path": "/var/log/spooler",
            "log_group_name": "/app/HOST_CLASS/system/spooler",
            "log_stream_name": "HOST_CLASS"
          }
        ]
      }
    }
  },
  "metrics": {
    "append_dimensions": {
      "AutoScalingGroupName": "${aws:AutoScalingGroupName}",
      "ImageId": "${aws:ImageId}",
      "InstanceId": "${aws:InstanceId}",
      "InstanceType": "${aws:InstanceType}"
    },
    "metrics_collected": {
      "collectd": {
        "metrics_aggregation_interval": 60
      },
      "disk": {
        "measurement": [
          "used_percent"
        ],
        "metrics_collection_interval": 60,
        "resources": [
          "*"
        ]
      },
      "mem": {
        "measurement": [
          "mem_used_percent"
        ],
        "metrics_collection_interval": 60
      },
      "statsd": {
        "metrics_aggregation_interval": 60,
        "metrics_collection_interval": 10,
        "service_address": ":8125"
      }
    }
  }
}
